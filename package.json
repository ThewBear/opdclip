{
  "name": "opdclip",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "deploy": "netlify deploy --dir=./dist",
    "publish": "netlify deploy --prod --dir=./dist",
    "format": "prettier --write \"**/*.{astro,ts,js,mjs,json,css}\"",
    "format:check": "prettier --check \"**/*.{astro,ts,js,mjs,json,css}\""
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "astro": "^5.14.5",
    "tailwindcss": "^4.1.14"
  },
  "license": "MIT",
  "devDependencies": {
    "netlify-cli": "^23.9.1",
    "prettier": "3.6.2",
    "prettier-plugin-astro": "0.14.1"
  }
}
